<%= turbo_frame_tag "preview", class: "h-full" do %>
  <iframe src="<%= profile_url(current_user) %>" 
  class="rounded-2xl h-full"
  style="border: 1rem solid #000;
    border-radius: 5%;
    padding: 10px;
    margin: 10px;"
  width="100%" 
  height="100%" 
  frameborder="0" 
  scrolling="yes"></iframe>
<% end %>
